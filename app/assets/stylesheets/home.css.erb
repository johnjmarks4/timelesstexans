html {
  width: 100%;
  width: 100%;
}

body {
  overflow: hidden;
}

#homepage {
  top: 80px;
  position: relative;
}

#title_div1 {
  width: 100%;
  height: 111px;
  position: relative;
  bottom: 120px;
  right: 8px;
}

#title_div2 {
  width: 75%;
