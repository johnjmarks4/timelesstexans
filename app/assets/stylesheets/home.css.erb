html {
  width: 100%;
}

body {
  overflow: hidden;
  width: 100%;
}

#homepage {
  top: 80px;
  position: relative;
}

#title_div1 {
  width: 100%;
  height: 111px;
  position: relative;
  bottom: 120px;
  right: 8px;
}

#title_div2 {
  width: 75%;
  height: 111px;
  position: relative;
  margin: 0 auto;
}

#title {
  font-family: "Chewy", sans-serif;
  src: url(/assets/chewy.ttf) format("truetype");
  font-size: 85px;
  text-align: center;
}

#left_stars {
  position: absolute;
  width: 234px;
  height: 12.1428px;
  top: 50%;
  transform: translateY(-50%);
}

#right_stars {
  position: absolute;
  right: 0px;
  width: 234px;
  height: 12.1428px;
  top: 50%;
  transform: translateY(-50%);
}

.star {
  display: inline-block;
  position: relative;
  height: 12.1428px;
}

#star1 {
  float: left;
}

#star2 {
  left: 50%;
  transform: translateX(-100%);
}

#star4 {
  left: 50%;
  transform: translateX(-150%);
}

#star3 {
  float: right;
}

#links {
  position: absolute;
  bottom: -56.8%;
  list-style: none;
  width: 101.2%;
  height: 120px;
  margin: 0;
  left: -8px;
  padding-top: 10px;
  padding-bottom: 10px;
  padding-left: 0;
  background-color: black;
}

#links > li {
  display: block;
  float: left;
  width: 10%;
  margin-left: 57.1px;
  padding-bottom: 5px;
}

.buttons, .java_hover {
  display: inline-block;
  background-size: 184.125px 120px;
  text-align: center;
  width: 184.125px;
  height: 120px;
  left: 5.7px;
  position: relative;
  margin: 2.9px;
  bottom: 2.9px;
}

.buttons a, .java_hover a {
  position: absolute;
  top: 40%;
  transform: translateX(-50%);
  text-decoration: none;
}

