}

#title {
  font-family: "Chewy", sans-serif;
  src: url(/assets/chewy.ttf) format("truetype");
  font-size: 85px;
  text-align: center;
}

#left_stars {
  position: absolute;
  width: 234px;
  height: 12.1428px;
  top: 50%;
  transform: translateY(-50%);
}

#right_stars {
  position: absolute;
  right: 0px;
  width: 234px;
  height: 12.1428px;
  top: 50%;
  transform: translateY(-50%);
}

.star {
  display: inline-block;
  position: relative;
  height: 12.1428px;
}

#star1 {
  float: left;
}

#star2 {
  left: 50%;
  transform: translateX(-100%);
}

#star4 {
  left: 50%;
  transform: translateX(-150%);
}

#star3 {
  float: right;
}

